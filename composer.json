{
	"name": "sherwinchia/livewire-image-uploader",
	"description": "Livewire upload image component.",
	"keywords": [
		"sherwinchia",
		"livewire",
		"image",
		"uploader",
		"laravel"
	],
	"homepage": "https://github.com/sherwinchia/livewire-image-uploader",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Sherwin Variancia",
			"email": "sherwinvariancia@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4|^8.0",
		"illuminate/support": "^8.0",
		"livewire/livewire": "^2.4"
	},
	"require-dev": {
		"orchestra/testbench": "^6.0",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Sherwinchia\\LivewireImageUploader\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Sherwinchia\\LivewireImageUploader\\Tests\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"test-coverage": "vendor/bin/phpunit --coverage-html coverage"
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Sherwinchia\\LivewireImageUploader\\LivewireImageUploaderServiceProvider"
			]
		}
	}
}
